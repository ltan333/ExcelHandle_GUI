<AnchorPane prefHeight="150.0" prefWidth="300.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.gui.minitask_gui.UpdatePopupController">
   <children>
      <Button fx:id="updateBtn" layoutX="171.0" layoutY="120.0" mnemonicParsing="false" text="Update" />
      <Label layoutX="57.0" layoutY="8.0" prefHeight="22.0" prefWidth="186.0" text="Choose week(s) to update">
