                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </ImageView>
                  <CheckBox fx:id="checkBox4" layoutX="19.0" layoutY="50.0" mnemonicParsing="false" selected="true" />
               </children>
            </AnchorPane>
         </children>
      </HBox>
      <Button fx:id="cancelBtn" layoutX="234.0" layoutY="120.0" mnemonicParsing="false" text="Cancel" />
      <CheckBox fx:id="checkBoxIsOpen" layoutX="14.0" layoutY="124.0" mnemonicParsing="false" selected="true" text="Open file after update" />
   </children>
</AnchorPane>
